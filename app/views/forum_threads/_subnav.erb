<div class="container">
	<%= search_form_for @q, html: { class: "form-inline category_id"} do |f| %>
	    <%= f.search_field :subject_cont, placeholder: "Search...", class: "form-control" %>
			<% ForumCategory.all.each do |forum_category| %>
	        	<%= f.check_box :forum_category_id_in, {multiple: true}, forum_category.id, nil %><%= forum_category.category_name %>
	      	<% end %>
	    <%= f.submit "Search", class: "btn btn-success" %>
	<% end %>
</div>